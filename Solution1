#include <stdio.h>

int main() {
    int t,n,s,d;
    scanf("%d",&t);
    while(t>0){
        s=50;
        d=0;
        scanf("%d",&n);
        // if(n==50){
        //     printf("%d",0);
        // }
            
        while(n!=50){
            if(n<50){
                if(n%2==0){
                    d=d+(50-n)/2;
                    break;
                }
                else{
                d=d+(53-n)/2 +1;
                break;
                }
            }  
            else{
                n=n-3;
                d++;
                
            }
            
        }
        printf("%d\n",d);
        t--;
    }
    return 0;
}
